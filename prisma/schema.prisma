generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model sensor_records {
  id            Int      @id @default(autoincrement())
  temperature   Decimal  @db.Decimal(5, 2)
  humidity      Decimal  @db.Decimal(5, 2)
  created_at    DateTime @db.Timestamp(6)
  device_name   String?
}
