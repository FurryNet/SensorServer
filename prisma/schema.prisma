generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model temp_records {
  id            Int      @id @default(autoincrement())
  temperature   Decimal  @db.Decimal(5, 2)
  humidity      Decimal  @db.Decimal(5, 2)
  record_ts     DateTime @db.Timestamp(6)
  location_name String?
}
